{
    "name": "thopra/styleguide-kickstart",
    "description": "Use this package as a kickstart to create a new style guide.",
    "repositories": [
        { "type": "vcs", "url": "https://github.com/thopra/styleguide.git" },
        { "type": "vcs", "url": "https://github.com/thopra/styleguide-template-default.git" },
        { "type": "vcs", "url": "https://github.com/thopra/styleguide-template-installer.git" }
    ],
    "license": "MIT",
    "require": {
        "thopra/styleguide":"*",
    	"thopra/styleguide-template-default":"*"
    },
    "authors": [
        {
            "name": "Thomas Prangenberg",
            "email": "t@thomas-prangenberg.de",
            "homepage": "http://www.thomas-prangenberg.de"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "target-dir": "styleguide"
}
